<h1>Create new User</h1>

<%= form_for(@user) do |form| %>
  <div class="field">
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password %>
  </div>

  <div class="action">
    <%= form.submit %>
  </div>
<% end %>